[user]
    name = Mark Blunk
    email = mblunk@gmail.com
[color]
    diff = auto
    status = auto
    branch = auto
[alias]
    st = status
    ci = commit
    br = branch
    co = checkout
    df = diff
    lg = log -p
    who = shortlog -s --
    slog  = !git --no-pager log --format=oneline --abbrev-commit
    dfnp  = !git --no-pager diff
    l = "!source ~/.githelpers && pretty_git_log"

[github]
    user = markblunk
[credential]
    helper = cache --timeout=3600
[core]
    editor = emacs
[push]
    default = simple